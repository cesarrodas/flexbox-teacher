{
  "name": "flexbox",
  "productName": "Flexbox Teacher",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build-linux": "electron-packager . Flexbox --prune=true",
    "build-linux-2": "electron-packager . Flexbox --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds",
    "build-mac": "electron-packager . Flexbox --overwrite --asar --platform=darwin --arch=x64 --icon=build/icon.icns --prune=true --out=release-builds",
    "build-win": "electron-packager . Flexbox --overwrite --asar=true --platform=win32 --arch=ia32 --icon=build/icon.ico --prune=true --out=release-builds"
  },
  "author": "cesar",
  "license": "ISC",
  "devDependencies": {
    "asar": "^0.13.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.13.0",
    "electron-packager": "^8.7.2"
  },
  "build": {
    "appId": "com.Cesar.FlexboxTeacher"
  }
}
